{
  "name": "codex-toolloop",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.6",
  "scripts": {
    "check": "bun run typecheck && bun run lint && bun run test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "fix": "biome format --write . && biome check --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "examples:basic": "bun examples/basic.ts",
    "examples:stream": "bun examples/stream.ts",
    "examples:structured": "bun examples/structured.ts",
    "examples:review": "bun examples/review.ts"
  },
  "dependencies": {
    "@openai/codex-sdk": "^0.87.0",
    "ai": "^6.0.42",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
