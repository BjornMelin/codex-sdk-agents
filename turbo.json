{
  "$schema": "https://turborepo.dev/schema.v2.json",
  "globalDependencies": [
    "package.json",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    "tsconfig.json"
  ],
  "globalEnv": ["NODE_ENV", "VERCEL_ENV", "CI"],
  "globalPassThroughEnv": ["CI", "GITHUB_TOKEN", "TURBO_TEAM", "TURBO_TOKEN"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "interactive": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "format": {
      "cache": false,
      "outputs": []
    },
    "fix": {
      "cache": false,
      "outputs": []
    },
    "codex:protocol:gen": {
      "outputs": []
    },
    "codex:schema:gen": {
      "outputs": []
    },
    "codex:gen": {
      "outputs": []
    },
    "codex:gen:check": {
      "cache": false,
      "outputs": []
    }
  }
}
